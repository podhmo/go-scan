{
  "openapi": "3.1.0",
  "info": {
    "title": "Sample API",
    "version": "0.0.1"
  },
  "paths": {
    "/pets/{petID}": {
      "get": {
        "description": "GetPet is a handler that uses a custom helper to get a path parameter.",
        "operationId": "custom-patterns_GetPet",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/users/{id}": {
      "get": {
        "description": "GetUser handles retrieving a user.",
        "operationId": "custom-patterns_GetUser",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/custom-patterns_User"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "custom-patterns_User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string"
          }
        }
      }
    }
  }
}
