{
  "info": {
    "title": "Sample API",
    "version": "0.0.1"
  },
  "openapi": "3.1.0",
  "paths": {
    "/settings": {
      "get": {
        "description": "GetSettingsHandler returns the current application settings.\nThis handler is designed to test map[string]any support.",
        "operationId": "GetSettingsHandler",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "options": {
                      "additionalProperties": {
                        "description": "unsupported basic type: any",
                        "type": "string"
                      },
                      "type": "object"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "OK"
          }
        }
      }
    },
    "/users/{id}": {
      "get": {
        "description": "GetUserHandler returns a user by ID.",
        "operationId": "GetUserHandler",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "OK"
          }
        }
      }
    }
  }
}
