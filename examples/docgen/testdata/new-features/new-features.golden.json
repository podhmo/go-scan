{
  "openapi": "3.1.0",
  "info": {
    "title": "Sample API",
    "version": "0.0.1"
  },
  "paths": {
    "/settings": {
      "get": {
        "description": "GetSettingsHandler returns the current application settings.\nThis handler is designed to test map[string]any support.",
        "operationId": "new-features_main_GetSettingsHandler",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/new-features_main_Settings"
                }
              }
            }
          }
        }
      }
    },
    "/things": {
      "post": {
        "description": "CreateThingHandler is for testing custom status code responses.",
        "operationId": "new-features_main_CreateThingHandler",
        "parameters": [
          {
            "name": "fail",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ]
      }
    },
    "/users/{id}": {
      "get": {
        "description": "GetUserHandler returns a user by ID.",
        "operationId": "new-features_main_GetUserHandler",
        "parameters": [
          {
            "name": "error",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "new-features_main_Settings": {
        "type": "object",
        "properties": {
          "options": {
            "type": "object",
            "additionalProperties": {
              "type": "string",
              "description": "unsupported basic type: any"
            }
          }
        }
      }
    }
  }
