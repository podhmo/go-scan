{
  "openapi": "3.1.0",
  "info": {
    "title": "Sample API",
    "version": "0.0.1"
  },
  "paths": {
    "/settings": {
      "get": {
        "description": "GetSettingsHandler returns the current application settings.\nThis handler is designed to test map[string]any support.",
        "operationId": "new-features_main_GetSettingsHandler",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/new-features_main_Settings"
                }
              }
            }
          }
        }
      }
    },
    "/users/{id}": {
      "get": {
        "description": "GetUserHandler returns a user by ID.",
        "operationId": "new-features_main_GetUserHandler",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/new-features_main_User"
                }
              }
            }
          },
          "default": {
            "description": "Default error response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/new-features_helpers_ErrorResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "new-features_helpers_ErrorResponse": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string"
          }
        }
      },
      "new-features_main_Settings": {
        "type": "object",
        "properties": {
          "options": {
            "type": "object",
            "additionalProperties": {
              "type": "string",
              "description": "unsupported basic type: any"
            }
          }
        }
      },
      "new-features_main_User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        }
      }
    }
  }
}
